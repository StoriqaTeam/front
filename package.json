  "scripts": {
    "build": "node scripts/build.js",
    "build:clean": "rm -rf ./build && mkdir ./build",
    "server:dist:clean": "rm -rf ./dist && mkdir ./dist",
    "server:dist:build": "yarn server:dist:clean && ./node_modules/.bin/webpack --config ./build-utils/webpack.server.production.js",
    "server": "yarn updateSchema && yarn relay && ./node_modules/.bin/babel-node ./server/index.js",
    "server:dev": "export NODE_ENV=\"development\" && export BABEL_ENV=\"development\" && export REACT_APP_HOST=\"https://localhost:3443\" && export REACT_APP_OAUTH_FACEBOOK_REDIRECT_URI=\"https://localhost:3443/oauth_callback/fb\" && export REACT_APP_OAUTH_GOOGLE_REDIRECT_URI=\"https://localhost:3443/oauth_callback/google\" && yarn server",
    "server:test": ". ./.env.test && yarn server",
    "server:prod": ". ./.env.stage && yarn build:clean && yarn build && yarn server:dist:build && node ./dist/server.js",
    "startSh": "./start.sh",
    "dev": "yarn dev:stable",
    "dev:nightly": ". ./.env.nightly && yarn startSh && yarn server:dev",
    "dev:stable": ". ./.env.stable && yarn startSh && yarn server:dev",
    "dev:stage": ". ./.env.stage && yarn startSh && yarn server:dev",
    "ci": "yarn lint && yarn flow && yarn test",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "NODE_ENV=test BABEL_ENV=test jest",
    "storybook": ". ./.env.stable && NODE_ENV=development BABEL_ENV=development yarn updateSchema && start-storybook -p 9000 -c .storybook",
    "relay": "./node_modules/.bin/relay-compiler --src ./src --schema ./src/relay/schema.json",
    "updateSchema": "./node_modules/.bin/babel-node scripts/updateSchema.js",
    "checkImage": "cp docker/Dockerfile.build Dockerfile && docker build -t fronttest . && rm Dockerfile",
    "prettier": "prettier --single-quote --trailing-comma=all --write './src/**/*.js'",
    "buildTranslations": "NODE_ENV=development yarn babel-node ./scripts/i18n/buildTranslations.js",
    "validateTranslations": "NODE_ENV=development yarn babel-node ./scripts/i18n/validateTranslations.js",
    "cypress:dev": "CYPRESS_baseUrl=https://localhost:3443 CYPRESS_graphql=https://nightly.stq.cloud/graphql $(yarn global bin)/cypress open",
    "cypress:dev:nightly": "CYPRESS_baseUrl=https://nightly.stq.cloud CYPRESS_graphql=https://nightly.stq.cloud/graphql $(yarn global bin)/cypress open",
    "cypress:docker": "docker run -i --rm -e \"CYPRESS_graphql=https://stage.stq.cloud/graphql\" -v $PWD/cypress.json:/app/cypress.json -v $PWD/cypress:/app/cypress storiqateam/front_cypress"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
