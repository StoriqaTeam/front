  },
  "peerDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "farce": "^0.2.5"
  },
  "scripts": {
    "env:dev": "touch .env.development && rm .env.development && cp .env.dev-manual .env.development",
    "env:docker": "touch .env.development && rm .env.development && cp .env.dev-docker .env.development",
    "build": "node scripts/build.js",
    "server:prod": "yarn build && NODE_ENV=production BABEL_ENV=production ./node_modules/.bin/babel-node ./server/index.js",
    "server:dev": "yarn updateSchema && NODE_ENV=development BABEL_ENV=development ./node_modules/.bin/babel-node ./server/index.js",
    "dev": "yarn env:dev && yarn server:dev",
    "docker": "yarn env:docker && cd docker && docker-compose up",
    "ci": "yarn lint && yarn flow",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "jest",
    "storybook": "yarn updateSchema && start-storybook -p 9001 -c .storybook",
    "updateSchema": "BABEL_ENV=development NODE_ENV=development ./node_modules/.bin/babel-node scripts/updateSchema.js"
  },
  "prepush": "ci",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
